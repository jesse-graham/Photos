<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="745.0" prefWidth="1130.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SlideshowView">
   <children>
      <ImageView fx:id="imageView" fitHeight="749.0" fitWidth="1130.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="957.0" layoutY="33.0" mnemonicParsing="false" onAction="#returnToAlbumView" prefHeight="60.0" prefWidth="136.0" text="Return">
         <font>
            <Font size="32.0" />
         </font></Button>
      <Button fx:id="prevPhotoButton" layoutX="14.0" layoutY="350.0" mnemonicParsing="false" onAction="#previousPhoto" prefHeight="60.0" prefWidth="106.0" text="Prev">
         <font>
            <Font size="32.0" />
         </font></Button>
      <Button fx:id="nextPhotoButton" layoutX="994.0" layoutY="335.0" mnemonicParsing="false" onAction="#nextPhoto" text="Next">
         <font>
            <Font size="32.0" />
         </font></Button>
      <Button layoutX="754.0" layoutY="33.0" mnemonicParsing="false" onAction="#quitApp" text="Button">
         <font>
            <Font size="32.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
