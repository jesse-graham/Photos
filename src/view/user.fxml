<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="663.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserController">
   <AnchorPane prefHeight="173.0" prefWidth="606.0">
      <children>
         <Label fx:id="Username" layoutX="12.0" layoutY="14.0" text="User Dashboard">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
         <Button fx:id="logOutButton" layoutX="533.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOutButton" text="LogOut" />
         <TextField fx:id="albumTextField" layoutX="98.0" layoutY="64.0" prefHeight="33.0" prefWidth="165.0" promptText="Album Name" />
         <Button fx:id="searchAlbums" layoutX="287.0" layoutY="68.0" mnemonicParsing="false" onAction="#searchButton" text="Search" />
         <Button fx:id="addAlbum" layoutX="121.0" layoutY="126.0" mnemonicParsing="false" onAction="#addAlbumButton" text="Add Album" />
         <Button fx:id="deleteAlbum" layoutX="223.0" layoutY="125.0" mnemonicParsing="false" onAction="#deleteAlbumButton" text="Delete Album" />
         <Button fx:id="openAlbum" layoutX="49.0" layoutY="125.0" mnemonicParsing="false" onAction="#openButton" text="Open" />
         <Button fx:id="cancelSearch" layoutX="373.0" layoutY="68.0" mnemonicParsing="false" onAction="#cancelSearchButton" text="Cancel Search" />
         <Button fx:id="renameAlbum" layoutX="341.0" layoutY="126.0" mnemonicParsing="false" onAction="#renameButton" text="Rename" />
         <Button fx:id="searchPhotos" layoutX="436.0" layoutY="125.0" mnemonicParsing="false" onAction="#searchPhotosButton" text="Search Photos" />
         <Button layoutX="457.0" layoutY="15.0" mnemonicParsing="false" onAction="#quitApp" text="Quit" />
      </children>
   </AnchorPane>
   <ListView fx:id="albumList" layoutX="-1.0" layoutY="172.0" prefHeight="491.0" prefWidth="606.0" />
</AnchorPane>
